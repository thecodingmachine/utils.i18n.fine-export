    "keywords": ["i18n", "internationalisation", "fine", "mouf", "export"],
    "homepage": "https://github.com/thecodingmachine/utils.i18n.fine-export",
    "type": "mouf-library",
    "license": "MIT",
    "authors": [
        {
            "name": "Marc Teyssier",
            "email": "m.teyssier@thecodingmachine.com",
            "homepage": "http://mouf-php.com"
        }
        
    ],
    "require": {
    	"php": ">=5.3.0",
    	"mouf/mouf-installer": "~2.0",
    	"mouf/utils.i18n.fine" : "~3.0",
    	"phpexcel/phpexcel" : ">=1.7.6, <2.0.0"
    },
    "autoload": {
        "psr-0": {
            "Mouf": "src/"
        }
